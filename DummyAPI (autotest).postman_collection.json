			"name": "CreatePost",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200 OK\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"Status code is OK\", function () {\r",
							"    pm.response.to.have.status(\"OK\");\r",
							"});\r",
							"\r",
							"pm.test(\"Response time is less than 500ms\", function () {\r",
							"    pm.expect(pm.response.responseTime).to.be.below(1000);\r",
							"});\r",
							"\r",
							"// Сначала создаем переменную jsonData с телом ответа\r",
							"var jsonData = pm.response.json();\r",
							"\r",
							"// Затем создаем параметр postID, который берет значение из тела ответа (сгенерированное \"id\"), чтобы потом он подставлялся в других запросах\r",
							"\r",
							"pm.collectionVariables.set(\"postId\", jsonData.id);\r",
							"\r",
							"// Создаем тесты для проверки соответствия значений ключей в запросе и ответе\r",
							"\r",
							"pm.test(\"Check body image\", function () {\r",
							"    pm.expect(jsonData.image).to.eql(\"myphoto.ru\");\r",
							"});\r",
							"\r",
							"pm.test(\"Check body text\", function () {\r",
							"    pm.expect(jsonData.text).to.eql(\"Hello\");\r",
							"});\r",
							"\r",
							"pm.test(\"Check body likes\", function () {\r",
							"    pm.expect(jsonData.likes).to.eql(10);\r",
							"});\r",
							"\r",
							"pm.test(\"Check body tags\", function () {\r",
							"    pm.expect(jsonData.tags).to.eql([\"dogs\"]);\r",
							"});\r",
							"\r",
							"pm.test(\"Check body owner\", function () {\r",
							"    pm.expect(jsonData.owner.id).to.eql(\"6809f77090817d7caba036b3\");\r",
							"});\r",
							"\r",
							"// Проверяем значение string у ключей, которые сами генерируются (id, publishDate, uodatedDate)\r",
							"\r",
							"pm.test(\"Check body post id is string\", function () {\r",
							"pm.expect(jsonData.id).to.be.a('string');\r",
							"});\r",
							"\r",
							"pm.test(\"Check body publishDate is string\", function () {\r",
							"pm.expect(jsonData.publishDate).to.be.a('string');\r",
							"});\r",
							"\r",
							"pm.test(\"Check body updatedDate is string\", function () {\r",
							"pm.expect(jsonData.updatedDate).to.be.a('string');\r",
							"});\r",
							"\r",
							"pm.test(\"Check body owner is object\", function () {\r",
							"pm.expect(jsonData.owner).to.be.a('object');\r",
							"});\r",
							"\r",
							"pm.test(\"Check body owner include 5 keys\", function () {\r",
							"pm.expect(Object.keys(jsonData.owner).length).to.equal(5);\r",
							"});"
						],
